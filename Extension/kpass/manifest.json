{

    "name":"Kpass",
    "description":"button click",
    "version":"1.0",
    "author":"abdoul",
    "background":{
        "scripts":["background.js"]
    },
"content_scripts":[
    {
        "matches":["<all_urls>"],
        "js":["content.js"]
    }
    ],
    "browser_action":{
        "default_popup":"popup.html",
        "default_title": "Sign in and save your passwords safely"
    },
    
    "permissions":[
        "storage",
        "tabs",
        "<all_urls>",
        "webRequest",
        "webRequestBlocking"
    ],
    "manifest_version": 2

}